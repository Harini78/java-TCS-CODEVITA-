import java.util.*;

public class Main {
    public static void main(String[] args) {
        int a = 1;
        int b = 2;
        int c = 0;
        int n = 5;

        for (int i = 0 ; i < n - 2; i++) {
            c = a + b;
            a = b;
            b = c;
        }

        System.out.println(c);
    }
}

